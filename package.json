{
  "name": "gis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open --mode development",
    "serve": "vue-cli-service serve --open --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:prod": "vue-cli-service build --mode production",
    "deploy:dev": "npm run build:dev && cross-env NODE_ENV=development node ./deploy",
    "deploy:prod": "npm run build:prod && cross-env NODE_ENV=production node ./deploy",
    "lint": "vue-cli-service lint",
    "deletePakage": "rimraf node_modules"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "postcss-pxtorem": "^5.1.1",
    "sass-loader": "^9.0.2",
    "scp2": "^0.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
